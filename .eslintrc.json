{
	"root": true,
	"parser": "@typescript-eslint/parser",
	"plugins": [
		"@typescript-eslint"
	],
	"parserOptions": {
		"sourceType": "module",
		"ecmaVersion": "2020",
		"ecmaFeaures": {
			"jsx": true,
			"modules": true
		}
	},
	"extends": [
		"eslint:recommended",
		"plugin:@typescript-eslint/eslint-recommended",
		"plugin:@typescript-eslint/recommended"
	],
	"rules": {
		"no-console": "warn",
		"no-mixed-spaces-and-tabs": "off"'
	}
}
